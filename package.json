{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node ./bin/index.js",
    "dev": "nodemon ./bin/index.js",
    "build": "npm run db:generate && npm run db:push:prod",
    "deploy-setup": "node deploy-setup.js",
    "test-absent": "node test-absent-automation.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:prod": "set NODE_ENV=production && prisma migrate deploy",
    "db:push": "prisma db push",
    "db:push:prod": "set NODE_ENV=production && prisma db push",
    "db:studio": "prisma studio",
    "db:studio:prod": "set NODE_ENV=production && prisma studio",
    "db:reset": "prisma migrate reset",
    "db:resolve:prod": "set NODE_ENV=production && prisma migrate resolve --applied 20250806104631_init_postgresql"
  },
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^1.41.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "date-fns": "^4.1.0",
    "debug": "~2.6.9",
    "dotenv": "^17.2.0",
    "express": "^4.21.2",
    "http": "^0.0.1-security",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^9.0.2",
    "moment-timezone": "^0.6.0",
    "morgan": "~1.9.1",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^4.0.0",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.5",
    "nodemon": "^3.1.10",
    "resend": "^4.7.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "prisma": "^6.12.0"
  }
}
